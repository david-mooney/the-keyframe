{
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "cSpell.words": [
    "frontmatter",
    "iframe",
    "monospace",
    "multiline",
    "paarl",
    "prismjs",
    "wesbos"
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/package-lock.json": true
  }
}
